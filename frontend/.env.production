# Production environment variables for STABLE-FUNDS

# Disable simulation in production
REACT_APP_SIMULATE_SUCCESS=false

# Use the mainnet or devnet cluster for Solana transactions
REACT_APP_SOLANA_CLUSTER=devnet

# Log server endpoint (if needed in production)
REACT_APP_LOG_ENDPOINT=

# Disable mock data in production
REACT_APP_USE_MOCKS=false

# Your app-specific environment variables
VITE_API_URL=https://api.stablefunds.app/api
VITE_STABLEFUNDS_PROGRAM_ID=YOUR_STABLEFUNDS_PROGRAM_ID_HERE
VITE_STABLEFUND_ID=YOUR_STABLEFUND_ID_HERE

# Network settings
VITE_SOLANA_NETWORK=mainnet

# Feature flags
VITE_FEATURE_JITO=true
VITE_FEATURE_GOVERNANCE=true
VITE_FEATURE_STABLECOIN=true

# Civic Auth
VITE_CIVIC_CLIENT_ID=adf20b1e-3021-4ec2-96ea-b0293f774344